{% extends "layout.twig" %}

{% block content %}

<h2>Users</h2>
<p>
	<a class="btn btn-primary" href="{{ 'admin/users/create' | url }} ">New User</a>
</p>
<table class="table">
	<tr>
		<th>Name</th>
		<th>Email</th>
		<th>Edit</th>
		<th>Delete</th>
	</tr>
	{% for user in users %}
	<tr>
		<td>{{ user.name }}</td>
		<td>{{ user.email }}</td>
		<td>Edit</td>
		<td>Delete</td>
	</tr>
	{% endfor %}
</table>

{% endblock %}